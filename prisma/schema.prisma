// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Vianda {
  id Int @id @default(autoincrement())
  nombre String
  tipo Tipo
  descripcion String
  ingredientes String
  imagen String
}

enum Tipo{
  clasico
  sinHarinas
  vegetariano
  dieta
}